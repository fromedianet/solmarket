{
  "name": "@oyster/common",
  "version": "0.0.2",
  "description": "Oyster common utilities",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": "./dist/lib",
    "./dist/*": "./dist/*.js",
    "./styles.css": "./dist/lib/styles.css"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "run-s build-ts build-css",
    "build-ts": "tsc",
    "build-css": "less-watch-compiler src/ dist/lib/ --run-once",
    "start": "npm-run-all --parallel watch watch-css watch-css-src",
    "watch-css": "less-watch-compiler src/ dist/lib/",
    "watch-css-src": "less-watch-compiler src/ src/",
    "watch": "tsc --watch",
    "test": "jest test --passWithNoTests",
    "clean": "rm -rf dist",
    "prepare": "run-s clean build",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\""
  },
  "dependencies": {
    "@metaplex/arweave-cost": "^1.0.4",
    "@project-serum/serum": "^0.13.64",
    "@solana/spl-token": "0.1.6",
    "@solana/spl-token-registry": "0.2.202",
    "@solana/wallet-adapter-base": "^0.4.1",
    "@solana/wallet-adapter-react": "^0.7.2",
    "@solana/wallet-adapter-wallets": "^0.6.2",
    "@solana/web3.js": "^1.33.0",
    "antd": "^4.20.0-alpha.1",
    "bn.js": "^5.2.0",
    "borsh": "^0.7.0",
    "bs58": "^5.0.0",
    "buffer-layout": "^1.2.2",
    "eventemitter3": "^4.0.7",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-toastify": "^8.2.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/mime": "^2.0.3",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/react-router-dom": "^5.3.3",
    "less": "^4.1.2",
    "less-watch-compiler": "1.16.3",
    "prettier": "^2.6.2"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
